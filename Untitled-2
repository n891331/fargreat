        <div class="r t-rec" data-record-type="270" id="rec101503541" style=" ">
            <div class="t270"></div>
            <script type="text/javascript">
                $(document).ready(function () {
                    setTimeout(function () {
                        var $root = $('html, body');
                        $('a[href*="#"]:not([href="#"],.carousel-control,.t-carousel__control,[href^="#price"],[href^="#popup"],[href^="#prodpopup"],[href^="#order"])').click(function () {
                            var target = $(this.hash);
                            if (target.length == 0) {
                                target = $('a[name="' + this.hash.substr(1) + '"]');
                            }
                            if (target.length == 0) {
                                return true;
                            }
                            $root.animate({
                                scrollTop: target.offset().top + 3
                            }, 500);
                            return false;
                        });
                    }, 500);
                });
            </script>
