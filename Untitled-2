            <div class="t-container_100 ">
                <div class="t-width t-width_100 ">
                    <script>
                        $(document).ready(function () { // Copy-color-to-clipboard
    
                            $("a[href='#copy-color']").attr('onclick', 'copyColor(event)').addClass('copy-color-btn').removeAttr('href');
    
                        });
    
                        function copyColor(e) {
                            e.preventDefault();
                            var copyText = document.getElementById("pantone-color");
                            copyText.select();
                            document.execCommand("copy");
                            $(".copy-color-btn").text('copied');
                        }
                    </script>
                    <div style="overflow: hidden; display: block; height: 1px; width: 1px">
                        <input id="pantone-color" type="text" value="#ff6f61">
                    </div>
                </div>
            </div>
